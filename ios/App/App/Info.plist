	<array>
		<dict>
			<key>CFBundleURLName</key>
			<string>me.newomen.app</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>newomen</string>
			</array>
		</dict>
	</array>
	<key>LSApplicationQueriesSchemes</key>
	<array>
		<string>sms</string>
		<string>tel</string>
		<string>mailto</string>
		<string>https</string>
		<string>http</string>
	</array>
</dict>
</plist>
