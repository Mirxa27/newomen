<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Push Notifications -->
	<key>aps-environment</key>
	<string>production</string>
	
	<!-- Background Modes -->
	<key>UIBackgroundModes</key>
	<array>
		<string>remote-notification</string>
		<string>fetch</string>
		<string>processing</string>
	</array>
	
	<!-- Keychain Sharing -->
	<key>keychain-access-groups</key>
	<array>
		<string>$(AppIdentifierPrefix)com.newomen.app</string>
	</array>
	
	<!-- Associated Domains -->
	<key>com.apple.developer.associated-domains</key>
	<array>
		<string>applinks:newomen.me</string>
		<string>webcredentials:newomen.me</string>
	</array>
	
	<!-- HealthKit Sharing -->
	<key>com.apple.developer.healthkit</key>
	<true/>
	<key>com.apple.developer.healthkit.access</key>
	<array>
		<string>HKQuantityTypeIdentifierStepCount</string>
		<string>HKQuantityTypeIdentifierHeartRate</string>
		<string>HKQuantityTypeIdentifierBodyMass</string>
		<string>HKQuantityTypeIdentifierHeight</string>
		<string>HKQuantityTypeIdentifierActiveEnergyBurned</string>
		<string>HKQuantityTypeIdentifierSleepAnalysis</string>
		<string>HKCategoryTypeIdentifierMindfulSession</string>
	</array>
	
	<!-- Calendar and Contacts Access -->
	<key>com.apple.developer.calendar.access</key>
	<true/>
	<key>com.apple.developer.contacts</key>
	<true/>
	
	<!-- Video Calling Support -->
	<key>com.apple.developer.callkit</key>
	<true/>
	
	<!-- App Groups for Widgets/Extensions -->
	<key>com.apple.security.application-groups</key>
	<array>
		<string>group.com.newomen.app</string>
	</array>
	
	<!-- Data Protection -->
	<key>NSFileProtectionComplete</key>
	<true/>
</dict>
</plist>
